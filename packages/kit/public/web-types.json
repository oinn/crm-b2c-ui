{
  "$schema": "https://raw.githubusercontent.com/JetBrains/web-types/master/schema/web-types.json",
  "framework": "vue",
  "name": "CRM B2C UI Kit",
  "version": "0.1.0",
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "ui-app",
          "attributes": [
            {
              "name": "dark",
              "value": {
                "kind": "expression",
                "type": "boolean",
                "required": false
              }
            },
            {
              "name": "light",
              "value": {
                "kind": "expression",
                "type": "boolean",
                "required": false
              }
            }
          ]
        }
      ]
    }
  }
}
